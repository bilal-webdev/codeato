<link rel="stylesheet" href="/stylesheets/sign.css">

<div class="sign-width">
  <h3 class="text-center theme-color-dark font-weight-bold">Admin Pannel</h3>
  <br>
  <form class="container w-75" action="/adminsignin" method="POST">
    <div class="form-group">
      <input type="hidden" class="form-control" name="username" value="admin" id="exampleInputUsername" aria-describedby="usernameHelp">

      <input type="password" class="form-control" name="password" id="exampleInputUsername" aria-describedby="usernameHelp"
        placeholder="Password">
    </div>
    <Button type="submit" class="px20 btn btn-block theme-bgcolor text-dark font-weight-bold" data-toggle="modal"
      data-target="#modalRegisterForm">Sign In</Button>
  </form>
  <br>
</div>

